<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dimen_40"
    android:orientation="horizontal">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txt_item_id"
        style="@style/TextView12BoldBlack"
        android:layout_width="@dimen/dimen_60"
        android:layout_height="match_parent"
        android:layout_gravity="top"
        android:background="@drawable/table_header_right_cell_bg"
        android:gravity="center"
        android:paddingStart="@dimen/dimen_5"
        android:paddingEnd="@dimen/dimen_5"
        android:text="@string/id" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txt_item_team_name"
        style="@style/TextView12BoldBlack"
        android:layout_width="@dimen/dimen_200"
        android:layout_height="match_parent"
        android:layout_gravity="top"
        android:maxLines="1"
        android:background="@drawable/table_header_right_cell_bg"
        android:gravity="center"
        android:paddingStart="@dimen/dimen_5"
        android:paddingEnd="@dimen/dimen_5"
        android:text="Team Name" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txt_item_team_owner"
        style="@style/TextView12BoldBlack"
        android:layout_width="@dimen/dimen_200"
        android:layout_height="match_parent"
        android:layout_gravity="top"
        android:maxLines="1"
        android:background="@drawable/table_header_right_cell_bg"
        android:gravity="center"
        android:paddingStart="@dimen/dimen_5"
        android:paddingEnd="@dimen/dimen_5"
        android:text="Team Owner" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txt_item_spinner_status"
        style="@style/TextView12BoldBlack"
        android:layout_width="@dimen/dimen_150"
        android:layout_height="match_parent"
        android:layout_gravity="top"
        android:background="@drawable/table_header_right_cell_bg"
        android:gravity="center"
        android:paddingStart="@dimen/dimen_5"
        android:paddingEnd="@dimen/dimen_5"
        android:text="Entry Type/Status" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txt_item_spinner_category"
        style="@style/TextView12BoldBlack"
        android:layout_width="@dimen/dimen_150"
        android:layout_height="match_parent"
        android:layout_gravity="top"
        android:background="@drawable/table_header_right_cell_bg"
        android:gravity="center"
        android:paddingStart="@dimen/dimen_5"
        android:paddingEnd="@dimen/dimen_5"
        android:text="Entry Category" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/txt_item_player_name"
        style="@style/TextView12BoldBlack"
        android:layout_width="@dimen/dimen_200"
        android:layout_height="match_parent"
        android:layout_gravity="top"
        android:background="@drawable/table_header_right_cell_bg"
        android:gravity="center"
        android:paddingStart="@dimen/dimen_5"
        android:paddingEnd="@dimen/dimen_5"
        android:text="Players Name" />

</androidx.appcompat.widget.LinearLayoutCompat>