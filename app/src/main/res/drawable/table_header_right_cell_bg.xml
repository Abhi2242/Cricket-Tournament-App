<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:right="-2dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/white_shade_two" />

            <stroke
                android:width="1dp"
                android:color="@color/grey_two" />
        </shape>
    </item>
    <item
        android:bottom="-2dp"
        android:left="-2dp"
        android:top="-2dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/white_shade_two" />

            <stroke
                android:width="1dp"
                android:color="@color/white" />
        </shape>
    </item>
</layer-list>
